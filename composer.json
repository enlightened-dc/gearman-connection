{
  "name": "tm/gearman-connection",
  "description": "A simple and clean library to get informations from your gearman jobserver.",
  "keywords": [
    "gearman",
    "connection",
    "gearman-connection",
    "jobserver"
  ],
  "homepage": "https://github.com/tommy-muehle/php-gearman-connection",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tommy Muehle",
      "email": "tommy.muehle@gmail.com"
    },
    {
      "name": "Michael Schindler",
      "email": "mi-schi@gmx.net"
    }
  ],
  "support": {
    "email": "tommy.muehle@gmail.com",
    "issues": "https://github.com/tommy-muehle/php-gearman-connection/issues",
    "docs": "https://github.com/tommy-muehle/php-gearman-connection/blob/master/README.md",
    "source": "https://github.com/tommy-muehle/php-gearman-connection/tree/master"
  },
  "require": {
    "php": "^5.4.0|~7.0",
    "ext-gearman": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": { "TM\\Gearman\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "TM\\Gearman\\Tests\\": "tests/" }
  },
  "config": {
    "bin-dir": "bin"
  }
}
